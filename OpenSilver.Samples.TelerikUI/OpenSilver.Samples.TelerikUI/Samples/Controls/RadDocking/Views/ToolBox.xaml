<UserControl x:Class="OpenSilver.Samples.TelerikUI.Docking.ToolBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             mc:Ignorable="d"
             d:DesignHeight="300"
             d:DesignWidth="300">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition />
        </Grid.RowDefinitions>

        <telerik:RadWatermarkTextBox WatermarkContent="Search Toolbox"
                                     Margin="5" />

        <telerik:RadTreeView Grid.Row="1"
                             Margin="5"
                             VerticalAlignment="Stretch"
                             Background="{DynamicResource LightGrayBackgroundBrush}"
                             IsLineEnabled="False"
                             SelectionMode="Multiple"
                             IsEditable="True"
                             IsDragDropEnabled="True">
            <telerik:RadTreeViewItem Header="Telerik Input 2012 Q2"
                                     IsExpanded="true">
                <telerik:RadTreeViewItem Header="RadPdfViewer"
                                         DefaultImageSrc="/OpenSilver.Samples.TelerikUI;component/Samples/Controls/RadDocking/Images/Docking/Toolbox/pdfviewer.png" />
                <telerik:RadTreeViewItem Header="RadImageEditor"
                                         DefaultImageSrc="/OpenSilver.Samples.TelerikUI;component/Samples/Controls/RadDocking/Images/Docking/Toolbox/imageeditor.png" />
                <telerik:RadTreeViewItem Header="RadRichTextBox"
                                         DefaultImageSrc="/OpenSilver.Samples.TelerikUI;component/Samples/Controls/RadDocking/Images/Docking/Toolbox/richtextbox.png" />
            </telerik:RadTreeViewItem>

            <telerik:RadTreeViewItem Header="Telerik Visualization 2012 Q2"
                                     IsExpanded="true">
                <telerik:RadTreeViewItem Header="RadChartview"
                                         DefaultImageSrc="/OpenSilver.Samples.TelerikUI;component/Samples/Controls/RadDocking/Images/Docking/Toolbox/chartview.png" />
                <telerik:RadTreeViewItem Header="RadTimeline"
                                         DefaultImageSrc="/OpenSilver.Samples.TelerikUI;component/Samples/Controls/RadDocking/Images/Docking/Toolbox/timeline.png" />
                <telerik:RadTreeViewItem Header="RadTimebar"
                                         DefaultImageSrc="/OpenSilver.Samples.TelerikUI;component/Samples/Controls/RadDocking/Images/Docking/Toolbox/timebar.png" />
                <telerik:RadTreeViewItem Header="RadMap"
                                         DefaultImageSrc="/OpenSilver.Samples.TelerikUI;component/Samples/Controls/RadDocking/Images/Docking/Toolbox/map.png" />
                <telerik:RadTreeViewItem Header="RadAreaSparkline"
                                         DefaultImageSrc="/OpenSilver.Samples.TelerikUI;component/Samples/Controls/RadDocking/Images/Docking/Toolbox/sparklines.png" />
                <telerik:RadTreeViewItem Header="RadColumnSparkline"
                                         DefaultImageSrc="/OpenSilver.Samples.TelerikUI;component/Samples/Controls/RadDocking/Images/Docking/Toolbox/sparklines.png" />
                <telerik:RadTreeViewItem Header="RadLinearSparkline"
                                         DefaultImageSrc="/OpenSilver.Samples.TelerikUI;component/Samples/Controls/RadDocking/Images/Docking/Toolbox/sparklines.png" />
                <telerik:RadTreeViewItem Header="RadScatterSparkline"
                                         DefaultImageSrc="/OpenSilver.Samples.TelerikUI;component/Samples/Controls/RadDocking/Images/Docking/Toolbox/sparklines.png" />
                <telerik:RadTreeViewItem Header="RadWinLossSparkline"
                                         DefaultImageSrc="/OpenSilver.Samples.TelerikUI;component/Samples/Controls/RadDocking/Images/Docking/Toolbox/sparklines.png" />
                <telerik:RadTreeViewItem Header="RadPivot"
                                         DefaultImageSrc="/OpenSilver.Samples.TelerikUI;component/Samples/Controls/RadDocking/Images/Docking/Toolbox/pivotgrid.png" />
                <telerik:RadTreeViewItem Header="RadTreeMap"
                                         DefaultImageSrc="/OpenSilver.Samples.TelerikUI;component/Samples/Controls/RadDocking/Images/Docking/Toolbox/treemapandpivotmap.png" />
            </telerik:RadTreeViewItem>

            <telerik:RadTreeViewItem Header="Telerik Data 2012 Q2"
                                     IsExpanded="true">
                <telerik:RadTreeViewItem Header="RadDataFilter"
                                         DefaultImageSrc="/OpenSilver.Samples.TelerikUI;component/Samples/Controls/RadDocking/Images/Docking/Toolbox/datafilter.png" />
                <telerik:RadTreeViewItem Header="RadDataForm"
                                         DefaultImageSrc="/OpenSilver.Samples.TelerikUI;component/Samples/Controls/RadDocking/Images/Docking/Toolbox/dataform.png" />
                <telerik:RadTreeViewItem Header="RadDataPager"
                                         DefaultImageSrc="/OpenSilver.Samples.TelerikUI;component/Samples/Controls/RadDocking/Images/Docking/Toolbox/datapager.png" />
            </telerik:RadTreeViewItem>
        </telerik:RadTreeView>
    </Grid>

</UserControl>
