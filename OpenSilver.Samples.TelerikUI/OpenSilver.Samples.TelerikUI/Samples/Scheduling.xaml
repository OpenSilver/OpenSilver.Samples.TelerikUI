<UserControl
    x:Class="OpenSilver.Samples.TelerikUI.Samples.Scheduling"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:controlskit="clr-namespace:OpenSilver.ControlsKit;assembly=OpenSilver.ControlsKit.Controls"
    xmlns:local="clr-namespace:OpenSilver.Samples.TelerikUI">

    <StackPanel>
        <TextBlock Text="Scheduling" Style="{StaticResource PageHeader_Style}"/>
        <controlskit:StaggeredPanel DesiredColumnWidth="760" ProgressiveRenderingChunkSize="2">
            <local:RadScheduleView_Demo HorizontalAlignment="Center"/>
            <local:RadCalendar_Demo HorizontalAlignment="Center"/>
        </controlskit:StaggeredPanel>
    </StackPanel>
</UserControl>
